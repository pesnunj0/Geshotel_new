                    Catch ex As Exception

                    End Try
                    Dim DShuespedes As DataSet = getDataSet(cmd, sqlCabHuespedes)  'SELECT * FROM reservas_huespedes WHERE reserva_id=?

                    Dim y As Integer
                    For y = 0 To huespedes.Length - 1
                        Dim row As DataRow = Nothing
