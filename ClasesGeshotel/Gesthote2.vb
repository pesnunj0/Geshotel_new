            Dim z As New GesHotelClase(Me.userId)
            Dim limpiar As Boolean = False
            Dim cmd As MySqlCommand = Nothing
            If IsNothing(cmd) Then
                cmd = prepareConection(IsolationLevel.RepeatableRead)
