                        & "WHERE reservas_servicios.reserva_id = ? And reservas_servicios.unidad_calculo_id = 5"
                        filas(x)("bebes") = ExecuteScalar(cmd, sql)
                        sql = "Select Coalesce(abreviatura, '') From reservas Left Join reservas_tipo_habitacion ON " _
