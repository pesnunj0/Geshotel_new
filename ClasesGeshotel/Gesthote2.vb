                            row("cliente_id") = huespedes(y).cliente_id
                            row("fecha_llegada") = huespedes(y).fecha_llegada
                            row("fecha_salida") = DBNull.Value
                            If Not IsNothing(huespedes(y).fecha_salida) Then
