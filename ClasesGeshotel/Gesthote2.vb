                        tipounidadcalculo = "B"
                        tipo_huesped_id = 4
                    Case Dingus.PaxType.CHD ' Niños hay que preguntar la edad para saber si es N1 o N2
                        tipounidadcalculo = "N1"  ' Por defecto es N1 en caso de que no venga edad Cambiar?
                        tipo_huesped_id = 2
