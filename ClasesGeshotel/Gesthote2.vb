                            End If
                            row("habitacion_id") = DBNull.Value
                            If Not IsNothing(huespedes(y).habitacion) Then
                                huespedes(y).habitacion_id = ObtieneHabitacion_Id(cmd, hotel_id, huespedes(y).habitacion)
