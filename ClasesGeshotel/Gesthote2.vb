                    For z = 1 To filas.Length - 1
                        res("importe") += filas(z)("importe")
                        res("precio") += filas(z)("precio")
                        filas(z).Delete()
                    Next
                End If
            End While
            servicios = tmptable

            tmptable = servicios.Clone
            While servicios.Rows.Count - 1 >= 0
                Dim lin As DataRow = servicios.Rows(0)
